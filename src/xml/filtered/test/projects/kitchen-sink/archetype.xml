<p:properties
        name="Kitchen Sink"
        xml:lang="en"
        xmlns="http://www.w3.org/1999/xhtml"
        xmlns:p="${project.url}/xml/archetype-properties">
    <p:list>
        <p:artifactId>kitchen-sink</p:artifactId>
        <p:artifactName>Kitchen Sink</p:artifactName>
        <p:company>Company Inc.</p:company>
        <p:customClasspathScope>runtime</p:customClasspathScope>
        <p:disableWizard>true</p:disableWizard>
        <p:editions>enterprise standard</p:editions>
        <p:freeTrialPeriod>30</p:freeTrialPeriod>
        <p:ftpKeyStoreFile>ftp.ks</p:ftpKeyStoreFile>
        <p:ftpSecretMarkerClass>java.lang.Object</p:ftpSecretMarkerClass>
        <p:groupId>com.company.product</p:groupId>
        <p:hideUninstallAction>false</p:hideUninstallAction>
        <p:keyGenAuthorization>global.namespace.truelicense.api.LicenseManagementAuthorization.ALL</p:keyGenAuthorization>
        <p:keyGenValidation>new com.company.product.keygen.Main.NoLicenseValidation()</p:keyGenValidation>
        <p:keyGenValidationComposition>decorate</p:keyGenValidationComposition>
        <p:keyMgrAuthorization>global.namespace.truelicense.api.LicenseManagementAuthorization.ALL</p:keyMgrAuthorization>
        <p:keyMgrClock>java.time.Clock.systemDefaultZone()</p:keyMgrClock>
        <p:keyMgrValidation>new com.company.product.keymgr.Main.NoLicenseValidation()</p:keyMgrValidation>
        <p:keyMgrValidationComposition>decorate</p:keyMgrValidationComposition>
        <p:keyPairAlgorithm>EC</p:keyPairAlgorithm>
        <p:keyPairSize>571</p:keyPairSize>
        <p:keyStoreType>JKS</p:keyStoreType>
        <p:licenseKeyFormat>V2/XML</p:licenseKeyFormat>
        <p:package>com.company.product</p:package>
        <p:password>unsafe2020</p:password>
        <p:pbeAlgorithm>PBEWithMD5AndDES</p:pbeAlgorithm>
        <p:preferencesType>user</p:preferencesType>
        <p:privateKeyStoreFile>private.ks</p:privateKeyStoreFile>
        <p:publicKeyStoreFile>public.ks</p:publicKeyStoreFile>
        <p:signatureAlgorithm>SHA512withECDSA</p:signatureAlgorithm>
        <p:subject>StarGazer 2020</p:subject>
        <p:trueLicenseVersion>${truelicense.version}</p:trueLicenseVersion>
        <p:verboseCli>true</p:verboseCli>
        <p:version>1.0-SNAPSHOT</p:version>
    </p:list>
</p:properties>
