<p:properties
        name="Custom License Management Authorization"
        xml:lang="en"
        xmlns="http://www.w3.org/1999/xhtml"
        xmlns:p="${project.url}/xml/archetype-properties">
    <p:documentation>
        This example adds
        <code>keyGenAuthorization=global.namespace.truelicense.api.LicenseManagementAuthorization.ALL</code>
        and
        <code>keyMgrAuthorization=global.namespace.truelicense.api.LicenseManagementAuthorization.ALL</code>
        to the Basic Configuration example in order to implement a custom
        license authorization.
    </p:documentation>
    <p:list>
        <p:artifactId>custom-authorization</p:artifactId>
        <p:company>Company Inc.</p:company>
        <p:groupId>com.company.product</p:groupId>
        <p:keyGenAuthorization>global.namespace.truelicense.api.LicenseManagementAuthorization.ALL</p:keyGenAuthorization>
        <p:keyMgrAuthorization>global.namespace.truelicense.api.LicenseManagementAuthorization.ALL</p:keyMgrAuthorization>
        <p:password>test1234</p:password>
        <p:subject>Product 1</p:subject>
        <p:version>1.0-SNAPSHOT</p:version>
    </p:list>
</p:properties>
