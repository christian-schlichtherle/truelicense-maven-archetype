<p:properties
        name="Disable License Wizard"
        xml:lang="en"
        xmlns="http://www.w3.org/1999/xhtml"
        xmlns:p="${project.url}/xml/archetype-properties">
    <p:documentation>
        This example adds
        <code>disableWizard=true</code>
        to the Basic Configuration example in order to completely remove
        the license wizard dialog from the generated code.
    </p:documentation>
    <p:list>
        <p:artifactId>disable-wizard</p:artifactId>
        <p:company>Company Inc.</p:company>
        <p:disableWizard>true</p:disableWizard>
        <p:groupId>com.company.product</p:groupId>
        <p:password>test1234</p:password>
        <p:subject>Product 1</p:subject>
        <p:version>1.0-SNAPSHOT</p:version>
    </p:list>
</p:properties>
